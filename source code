totalbill_entry.pack(padx=100,pady=7)

total_button=Button(totalbill_frame,text='Total',font='lucida 13 bold',bg='orange',command=totalbutton)
total_button.pack(pady=5)

reset_button=Button(totalbill_frame,text='Reset',font='lucida 11 bold',bg='orange',command=resetbutton)
reset_button.pack(pady=5)
totalbill_frame.grid(row=1,column=8)

root.mainloop()

